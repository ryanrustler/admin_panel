<h1> Edit Cohorts </h1>

<%= form_with model: @cohort, url: cohort_path, method: :patch do |f| %>
<%= f.label :name %>
<%= f.text_field :name %><br> 

<%= f.label :start_date %>
<%= f.date_field :start_date %><br>

<%= f.label :end_date %>
<%= f.date_field :end_date %><br>

<%= f.label :create_cohort%>
<%= f.submit %>


<%= link_to 'Remove course', delete_course_path, method: :delete, data: { confirm: 'Are you sure?' } %>
<% end %>