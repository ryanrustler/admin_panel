<h1> Update Students</h1>

<%= form_with model: @student, url: student_path, method: :patch do |f| %>
<%= f.label :first_name %>
<%= f.text_field :first_name %><br> 

<%= f.label :last_name %>
<%= f.text_field :last_name %><br>

<%= f.label :age %>
<%= f.text_field :age %><br>

<%= f.label :education %>
<%= f.select(:education, ["HS", "B.D", "M.D", "Ph.D"]) %><br>

<%= f.label :create_student%>
<%= f.submit %>

<% end %>